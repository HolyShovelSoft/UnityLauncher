<UserControl x:Class="UnityLauncher.Core.EditorInfoOld"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             x:Name="This"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <StackPanel x:Name="Stack" Orientation="Horizontal">
            <Label  DataContext="{Binding ElementName=This}" Content="{Binding UIVersion}" FontWeight="Bold" Margin="0,-4"/>
            <Label  DataContext="{Binding ElementName=This}" Content="{Binding UIPath}" Margin="0,-4"/>
        </StackPanel>
    </Grid>
</UserControl>
